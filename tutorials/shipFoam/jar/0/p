FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      pd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Walls
    {
        type            buoyantPressure;
        value           uniform 0;
    }
    Heavy
    {
        type            zeroGradient;
    }
    Light
    {
        type            zeroGradient;
    }
    Top
    {
        type            fixedValue;
        value           uniform 0;
    }

}


