FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      pointMotionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Walls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Hull
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

	Top
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

}


