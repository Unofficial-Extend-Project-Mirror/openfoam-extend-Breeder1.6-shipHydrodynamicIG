FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      pd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    Top
    {
        type            totalPressure;
        p0              uniform 0;
        U               U;
        phi             phi;
        rho             rho;
        psi             none;
        gamma           0;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
    Outlet
    {
        type            zeroGradient;
    }
    Hull
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
