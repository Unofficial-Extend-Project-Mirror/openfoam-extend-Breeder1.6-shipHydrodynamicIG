FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha1 0
    volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        box (-1000 -1000 -1000) (1000 1000 0);

        fieldValues
        (
            volScalarFieldValue alpha1 1
        );
    }
);


